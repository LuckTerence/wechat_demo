{
  "name": "letwechat-uniapp-demo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "clean": "node -e \"const fs=require('node:fs');['.next','dist','unpackage','node_modules/.vite','node_modules/.vite-uni'].forEach((p)=>{try{fs.rmSync(p,{recursive:true,force:true})}catch{}})\"",
    "dev:h5": "uni -p h5",
    "dev:api": "node server/api-server.mjs",
    "build:h5": "uni build -p h5 && node -e \"console.log('H5 output: dist/build/h5')\"",
    "preview:h5": "node server/preview-h5.mjs",
    "build:preview:h5": "npm run build:h5 && npm run preview:h5"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-5000120260205002",
    "@dcloudio/uni-h5": "3.0.0-alpha-5000120260205002",
    "@dcloudio/uni-ui": "^1.5.11",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "vue": "3.4.21"
  },
  "devDependencies": {
    "@dcloudio/types": "3.4.28",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-5000120260205002",
    "sass": "^1.97.3",
    "typescript": "~5.8.2",
    "vite": "5.2.8"
  }
}
